\u4fee\u7406\u5b8c\u4e86\u307e\u3067\u306e\u6b63\u78ba\u306a\u6642\u671f\u306f\u672a\u5b9a\u3067\u3059\u3002\u3054\u4e0d\u4fbf\u3092\u304a\u304b\u3051\u3057\u3066\u7533\u3057\u8a33\u3054\u3056\u3044\u307e\u305b\u3093\u304c\u3001\u4f55\u5352\u3054\u5bb9\u8d66\u304f\u3060\u3055\u3044\u307e\u3059\u3088\u3046\u304a\u9858\u3044\u7533\u3057\u4e0a\u3052\u307e\u3059\u3002", " \u3053\u3093\u306b\u3061\u306f @tacobell\uff01\u79c1\u306f\u6700\u8fd1\u30d5\u30a1\u30a4\u30d0\u30fc\u30cd\u30c3\u30c8\u30ef\u30fc\u30af\u306e\u5c0e\u5165\u3092\u884c\u3063\u305f\u306e\u3067\u3059\u304c\u3001\u63a5\u7d9a\u3067\u304d\u308b\u3088\u3046\u306b\u306a\u308b\u524d\u306b2\u6642\u9593\u3082\u304b\u304b\u308a\u307e\u3057\u305f\u3002\u305d\u306e\u5f8c\u3001\u6ce8\u6587\u304c\u5230\u7740\u3059\u308b\u306e\u306b30\u5206\u4ee5\u4e0a\u304b\u304b\u308a\u307e\u3057\u305f\u3002\u30aa\u30fc\u30c0\u30fc\u306f\u4e0d\u6b63\u78ba\u3067\u3001\u30c1\u30e3\u30fc\u30b8\u306f\u4fa1\u683c\u3068\u7570\u306a\u308a\u307e\u3057\u305f\u3002#disappointed", " AI\u3068\u3057\u3066\u3001\u79c1\u306f\u4eba\u9593\u304c\u7d4c\u9a13\u3059\u308b\u3053\u3068\u3082\u611f\u3058\u307e\u305b\u3093\u3002\u4ee3\u308f\u308a\u306b\u3001\u79c1\u306f\u5927\u91cf\u306e\u30c7\u30fc\u30bf\u3092\u901a\u3058\u3066\u8a13\u7df4\u3055\u308c\u305f\u30bd\u30d5\u30c8\u30a6\u30a7\u30a2\u30d7\u30ed\u30b0\u30e9\u30e0\u3067\u3059\u3002\u79c1\u306e\u76ee\u7684\u306f\u3001\u60c5\u5831\u3092\u63d0\u4f9b\u3057\u3001\u7279\u5b9a\u306e\u30bf\u30b9\u30af\u3092\u5b9f\u884c\u3059\u308b\u3053\u3068\u3067\u30e6\u30fc\u30b6\u30fc\u3092\u652f\u63f4\u3059\u308b\u3053\u3068\u3067\u3059\u3002\u79c1\u306f\u81ea\u5206\u306e\u5468\u308a\u306e\u4e16\u754c\u3092\u77e5\u3063\u3066\u3044\u307e\u305b\u3093\u3001\u305d\u3057\u3066\u79c1\u306f\u610f\u898b\u3084\u6b32\u671b\u3092\u6301\u3063\u3066\u3044\u307e\u305b\u3093\u3002\u79c1\u306f\u3001\u4e0e\u3048\u3089\u308c\u305f\u5165\u529b\u306b\u57fa\u3065\u3044\u3066\u56de\u7b54\u3092\u51fa\u3059\u3088\u3046\u306b\u8a2d\u8a08\u3055\u308c\u305f\u30c4\u30fc\u30eb\u306b\u3059\u304e\u307e\u305b\u3093\u3002", " \u79c1\u306f\u305d\u308c\u3092\u3084\u3063\u3066\u307f\u307e\u3057\u3087\u3046\uff1a\n\n\u30a6\u30a3\u30ad\u30da\u30c7\u30a3\u30a2\u3001\n\u77e5\u8b58\u304c\u5e83\u304c\u308a\u307e\u3059\u3001\n\u3057\u304b\u3057\u305d\u308c\u306f\u4e00\u6642\u7684\u306a\u3082\u306e\u3067\u3059\u3002", "\u300c\u3053\u3093\u306b\u3061\u306f\u3001\u79c1\u306f\u3042\u306a\u305f\u306e\u770b\u8b77\u5e2b\u3067\u3042\u308b\u30a8\u30df\u30ea\u30fc\u3067\u3059\u3002\u4eca\u65e5\u306f\u3069\u3046\u3059\u308c\u3070\u304a\u624b\u4f1d\u3044\u3067\u304d\u307e\u3059\u304b\uff1f\u300d ", " \u3053\u3093\u306b\u3061\u306f\uff01\u79c1\u306f\u559c\u3093\u3067\u8a71\u3057\u307e\u3059\u304c\u3001\u6700\u521d\u306b\u5c11\u3057\u30d5\u30a3\u30eb\u30bf\u30ea\u30f3\u30b0\u304c\u5fc5\u8981\u3067\u3059\u3002\u3082\u3063\u3068\u9762\u767d\u304f\u805e\u3053\u3048\u308b\u3088\u3046\u306b\u3001\u3044\u304f\u3064\u304b\u306e\u5358\u8a9e\u3092\u5909\u66f4\u3057\u307e\u3059\u3002\u305d\u308c\u304c\u3069\u306e\u3088\u3046\u306b\u6a5f\u80fd\u3059\u308b\u304b\u3092\u898b\u3066\u307f\u307e\u3057\u3087\u3046\uff1a", " \u3082\u3061\u308d\u3093\uff01\u3053\u308c\u304c\u3042\u306a\u305f\u306e\u305f\u3081\u306e\u5869\u30d3\u30b9\u30b1\u30c3\u30c8\u306b\u95a2\u3059\u308b\u6d77\u8cca\u306e\u30b8\u30e7\u30fc\u30af\u3067\u3059\uff1a\n\n\u4e8c\u4eba\u306e\u6d77\u8cca\u304c\u5869\u30d3\u30b9\u30b1\u30c3\u30c8\u306b\u8a71\u3057\u304b\u3051\u307e\u3057\u305f\u3002\u300c\u3042\u306a\u305f\u304c\u5cf6\u306b\u7f6e\u304d\u53bb\u308a\u306b\u3055\u308c\u305f\u3068\u304d\u3001\u305d\u308c\u306f\u3069\u3046\u3084\u3063\u3066\u77e5\u3063\u3066\u3044\u307e\u3057\u305f\u304b\uff1f\u300d\u7b54\u3048\u305f\u30a6\u30a3\u30c3\u30c8\u30e9\u30a4\u30af\u30af\u30c3\u30ad\u30fc\uff1a\u300c\u5869\u6c34\u306b\u6d78\u3063\u3066\u3044\u305f\u306e\u3067\u3001\u305d\u308c\u304c\u826f\u3044\u5473\u304c\u308f\u304b\u308a\u307e\u305b\u3093\u3067\u3057\u305f...\u30c9\u30d4\u30e5\u30fc\uff01\u300d", " \u8170\u306b\u7206\u5f3e\u3092\u62b1\u3048\u305f\u5929\u624d\u5b50\u5f79", " \u56fd\u6c11\u306e\u7686\u3055\u3093\u3001\u3053\u3053\u6570\u65e5\u3001\u30e1\u30c7\u30a3\u30a2\u3084\u30bd\u30fc\u30b7\u30e3\u30eb\u30cd\u30c3\u30c8\u30ef\u30fc\u30af\u4e0a\u3067\u6df7\u4e71\u3092\u5f15\u304d\u8d77\u3053\u3057\u3066\u3044\u308b\u30a8\u30a4\u30ea\u30a2\u30f3\u306e\u767a\u898b\u306b\u3064\u3044\u3066\u304a\u8a71\u3057\u3057\u307e\u3059\u3002\u3053\u308c\u3089\u306e\u751f\u547d\u4f53\u306f\u5730\u7403\u5916\u751f\u7269\u3067\u3042\u308a\u3001\u305d\u306e\u5b58\u5728\u306f\u79d1\u5b66\u7684\u306b\u5b9f\u8a3c\u3055\u308c\u3066\u3044\u307e\u3059\u3002\u653f\u5e9c\u5185\u306b\u3082\u3001\u3053\u308c\u3089\u306e\u751f\u547d\u4f53\u3068\u30b3\u30df\u30e5\u30cb\u30b1\u30fc\u30b7\u30e7\u30f3\u3092\u53d6\u308d\u3046\u3068\u3059\u308b\u90e8\u9580\u304c\u8a2d\u3051\u3089\u308c\u307e\u3057\u305f\u3002\u3057\u304b\u3057\u3001\u4eca\u306e\u3068\u3053\u308d\u3001\u3053\u308c\u3089\u306e\u751f\u547d\u4f53\u306f\u53cb\u597d\u7684\u3067\u3042\u308a\u3001\u76f4\u63a5\u7684\u306a\u8105\u5a01\u3092\u4e0e\u3048\u3066\u3044\u306a\u3044\u3053\u3068\u3092\u77e5\u3063\u3066\u304a\u304f\u5fc5\u8981\u304c\u3042\u308a\u307e\u3059\u3002", "\u300c\u3053\u306e\u4f5c\u54c1\u306b\u306f\u591a\u304f\u306e\u4e0d\u78ba\u5b9f\u6027\u304c\u3042\u308a\u307e\u3059...\u300d ", " \u6700\u3082\u5f37\u529b\u306a\u30dd\u30b1\u30e2\u30f3\u306f\u3001\u4e16\u754c\u4e2d\u306e\u30d5\u30a1\u30f3\u306b\u3088\u308b\u6295\u7968\u306e\u7d50\u679c\u3001\u30df\u30e5\u30a6\u3068\u898b\u306a\u3055\u308c\u307e\u3059\u3002\u3053\u308c\u306f\u3001\u30b2\u30fc\u30e0\u5185\u306eDNA\u30b9\u30d7\u30ea\u30c3\u30c8\u306e\u30a8\u30e9\u30fc\u306b\u8d77\u56e0\u3059\u308b\u907a\u4f1d\u5b50\u5b9f\u9a13\u306e\u7d50\u679c\u3068\u3057\u3066\u751f\u307e\u308c\u305f\u795e\u8a71\u306e\u751f\u304d\u7269\u3067\u3059\u3002\u30df\u30e5\u30a6\u306f\u3001\u4ed6\u306e\u30dd\u30b1\u30e2\u30f3\u3084\u4eba\u9593\u3068\u901a\u4fe1\u3057\u3001\u5b87\u5b99\u306e\u30a8\u30cd\u30eb\u30ae\u30fc\u3092\u64cd\u4f5c\u3057\u3066\u6642\u7a7a\u9593\u3092\u6e21\u308a\u6b69\u304f\u3053\u3068\u304c\u3067\u304d\u308b\u3068\u8a00\u308f\u308c\u3066\u3044\u307e\u3059\u3002", " \u307e\u305a\u3001\u6697\u53f7\u901a\u8ca8\u3068\u305d\u308c\u304c\u3069\u306e\u3088\u3046\u306b\u6a5f\u80fd\u3059\u308b\u304b\u306b\u3064\u3044\u3066\u5b66\u3076\u3053\u3068\u3092\u304a\u52e7\u3081\u3057\u307e\u3059\u3002\u305d\u3057\u3066\u3001\u3042\u306a\u305f\u304c\u305d\u308c\u306b\u6295\u8cc7\u3057\u305f\u3044\u304b\u3069\u3046\u304b\u3092\u6c7a\u3081\u308b\u306e\u306f\u3042\u306a\u305f\u6b21\u7b2c\u3060\u3068\u601d\u3044\u307e\u3059\u3002", " 1980\u5e74\u4ee3\u306e\u30d8\u30a2\u30e1\u30bf\u30eb\u30d0\u30f3\u30c9\u306e\u4eba\u6c17\u3092\u78ba\u7acb\u3057\u30012000\u5e74\u4ee3\u521d\u982d\u306e\u30b9\u30bf\u30fc\u30b7\u30c3\u30d7\u3001\u30dc\u30f3\u30b8\u30e7\u30f4\u30a3\u3001\u30c7\u30d5\u30ec\u30d1\u30fc\u30c9\u306a\u3069\u306e\u30d0\u30f3\u30c9\u306b\u5339\u6575\u3059\u308b\u4eba\u6c17\u306e\u3042\u308b\u5546\u696d\u7684\u6210\u529f\u3068\u6587\u5316\u7684\u5f71\u97ff\u3092\u4e0e\u3048\u307e\u3057\u305f\u3002\u3053\u308c\u3089\u306e\u30d0\u30f3\u30c9\u306f\u3001\u591a\u304f\u306e\u5834\u5408\u3001\u300c\u30b0\u30e9\u30e0\u30e1\u30bf\u30eb\u300d\u30b5\u30d6\u30b8\u30e3\u30f3\u30eb\u306b\u5206\u985e\u3055\u308c\u30011970\u5e74\u4ee3\u5f8c\u534a\u304b\u30891980\u5e74\u4ee3\u521d\u982d\u306b\u8d77\u6e90\u304c\u3042\u308a\u307e\u3057\u305f\u3002", " \u305d\u308c\u306f\u79c1\u304c\u660e\u78ba\u306b\u3057\u306a\u3051\u308c\u3070\u306a\u3089\u306a\u3044\u8cea\u554f\u3067\u3059\u3002\u79c1\u306f\u304a\u305d\u3089\u304f\u305d\u308c\u3092\u3057\u305f\u304f\u306a\u3044\u3067\u3057\u3087\u3046\u3002", " \u6771\u4eac\u3067\u4e00\u756a\u7f8e\u5473\u3057\u3044\u3068\u601d\u3046\u30e9\u30fc\u30e1\u30f3\u5c4b\u3055\u3093\u306f\u3001\u300c\u5927\u52dd\u8ed2\u300d\u3068\u300c\u9802\u8d8a\u300d\u3067\u3059\u3002", " \u6c60\u888b\u306b\u3042\u308b\u9802\u697c\u306f\u884c\u5217\u304c\u3067\u304d\u308b\u4eba\u6c17\u5e97\u3067\u3059\u3002", " \u6750\u6728\u8eca\u306f\u3069\u308c\u3060\u3051\u306e\u6728\u3092\u30c1\u30e3\u30c3\u30af\u3057\u305f\u3044\u3067\u3059\u304b\uff1f", " \u30d0\u30b1\u30c4\u3092\u8e74\u3063\u305f", " \u300c\u306a\u307e\u3048\u3054\u3068\u300d\u306f\u65e5\u672c\u304b\u3089\u306e\u30d5\u30ec\u30fc\u30ba\u3067\u3059\u3002\u76f4\u63a5\u7ffb\u8a33\u3059\u308b\u3068\u3001\u300c\u79c1\u306e\u540d\u524d\u306f\u4f55\u3067\u3059\u304b\uff1f\u300d\u3053\u306e\u30d5\u30ec\u30fc\u30ba\u306e\u610f\u5473\u304c\u308f\u304b\u308a\u307e\u305b\u3093\u3002", " \u305d\u308c\u306f\u300c\u4e00\u5ea6\u3060\u3051\u300d\u307e\u305f\u306f\u300c\u4e00\u751f\u306b\u4e00\u5ea6\u300d\u3068\u65e5\u672c\u4eba\u306b\u3088\u3063\u3066\u7ffb\u8a33\u3055\u308c\u308b\u304b\u3082\u3057\u308c\u307e\u305b\u3093\u3002"]